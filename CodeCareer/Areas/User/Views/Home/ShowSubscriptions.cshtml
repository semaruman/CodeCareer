@using CodeCareer.Areas.User.Services.Interfaces
@using CodeCareer.Areas.User.Services.Implementations.JsonServices

@inject IUserService UserService

@model UserModel



@{
	var subscriptions = UserService.GetUserModels().Where(u => Model.SubscriptionsEmails.Contains(u.Email));
}

<h1>Подписки <a asp-area="User" asp-controller="Home" asp-action="AlienProfile" asp-route-userEmail="@Model.Email">@Model.FullName</a></h1>

<div>
	@foreach (var sub in subscriptions)
	{
		<div>
			<a asp-area="User" asp-controller="Home" asp-action="AlienProfile" asp-route-userEmail="@sub.Email">@sub.FullName</a>
		</div>
	}
</div>